print("pierwszy projekt")

a = "pora roku"

b = "witaj!"
c = True
print(a, b, c)

g = "nastepna pora = jesien"

print("pora roku" + g + " " + b)

y = 11.7

print(6*y)

z ="4"
print(z)

z = eval(z)
print(4*z)

t = "lajkonik"

print(t)
print(t[0:3])           # od 0 do 2
print(t[3:7])           # od 3 do 6
print(t[0])
print(t[-7])            # 7 znak od końca
print(len(t))           #długość stringu
print(t[:-2])

g1 =10
g2 = 11

print(g1+g1,g1-g2,g1*g2,g1/g2,g1%g2)

print(round(10.54))     #zaokrąglenie

print(pow(5,2))         #potęga

u = 12
u += 5                  # u = u +5
i = 1
i += 1                  # i++;  i=i+1
p = 3 ** 2              # 3 do potęgi 2
print(i)
print(p)
